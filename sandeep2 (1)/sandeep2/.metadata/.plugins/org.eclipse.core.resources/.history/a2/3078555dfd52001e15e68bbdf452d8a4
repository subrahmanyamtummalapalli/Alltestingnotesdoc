package forLoops;

public class NumPrimeOrNot {

	public static void prime() {
		boolean status = false;
		int num = 13;
		for (int i = 2; i <= num/2; ++i) {
			if (num % i == 0) {
				status = true;
				break;

			}
		}
		if (status == false) 
			System.out.println(num );
		}

	

	public static void main(String[] args) {

		prime();

	}

}
