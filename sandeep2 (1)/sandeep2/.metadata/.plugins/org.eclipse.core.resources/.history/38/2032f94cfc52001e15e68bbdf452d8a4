package forLoops;

public class NumPrimeOrNot {
	
	public static void prime()
	{
		boolean status=true;
		 int num=13;
		for(int i=2;i<=13;i++)
		{
		if(num%i==0)
		{
			status=false;
			break;
			
		}
		}
		if(status==true)
		{
			System.out.println(num+"is a prime number");
		}
		
	}

	public static void main(String[] args) {
		
		prime();

	}

}
