package forLoops;

public class FibnociiSeries {
	
	public static void fibnoic()
	{
		int a=0,b=1,c;
		System.out.println(a);
		System.out.println(b);
		c=a+b;
		System.out.println(c);
		for(int i=0;i<=8;i++)
		{
		a=b;
		b=c;
		}
		System.out.println(a);
		System.out.println(b);
	}

	public static void main(String[] args) {
		
		fibnoic();
	}

}
