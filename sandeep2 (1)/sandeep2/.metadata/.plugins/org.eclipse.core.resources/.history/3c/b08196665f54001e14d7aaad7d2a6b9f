package ArraysConcept;

public class MissingNum {

	public static void main(String[] args) {

     boolean status=true;
     int a[]= {1,2,3,4,5,6,9};
     for(int j=1;j<=10;j++)
     {
    	 for(int i=0;i<a.length;i++)
    	 {
    		 if(j==a[i])
    		 {
    		 status=false;
    		 break;
    	 }
     }
     }
     if(status==true)
     {
    	 System.out.println(j);
     }

	}

}
